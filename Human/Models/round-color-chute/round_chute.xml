<?xml version="1.0"?>

<PropertyList>

 <path>round_chute.ac</path>
 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>10000</max-m>
 </animation>
 

<effect>
    <inherits-from>Aircraft/Y12B/Human/Effects/bumpspec-round_chute3obj</inherits-from>
    <object-name>round_chute3obj</object-name>
</effect>
<effect>
    <inherits-from>Aircraft/Y12B/Human/Effects/bumpspec-round_chute1obj</inherits-from>
    <object-name>round_chute1obj</object-name>
</effect>
<effect>
    <inherits-from>Aircraft/Y12B/Human/Effects/bumpspec-round_chute2obj</inherits-from>
    <object-name>round_chute2obj</object-name>
</effect>

<animation>
<name>RoundChute</name>
	<object-name>round_chute1obj</object-name>
	<object-name>round_chute2obj</object-name>
	<object-name>round_chute3obj</object-name>
</animation>


<animation>
		<type>scale</type>
		<object-name>RoundChute</object-name>
		<property>sim/walker/parachute-opened-sec</property>
		<x-factor>0.3</x-factor>
		<y-factor>0.25</y-factor>
		<z-factor>0.3</z-factor>
		<x-offset>0.2</x-offset>
		<y-offset>0</y-offset>
		<z-offset>0.2</z-offset>
		<x-min>0.2</x-min>
		<y-min>0.1</y-min>
		<z-min>0.2</z-min>
		<x-max>1.0</x-max>
		<y-max>1.0</y-max>
		<z-max>1.0</z-max>
	</animation>


 </PropertyList>
